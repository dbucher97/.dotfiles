{
  "snippets.userSnippetsDirectory": "~/.config/nvim/snippets/",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "javascriptreact",
    "java",
    "python"
  ],
  "diagnostic.errorSign": "ü§¨",
  "diagnostic.warningSign": "‚ö†Ô∏è",
  "diagnostic.hintSign": "üí°",
  "diagnostic.infoSign": "‚ùî",
  "diagnostic.joinMessageLines": true,
  "prettier.printWidth": 109,
  "prettier.trailingComma": "all",
  "prettier.singleQuote": true,
  "javascript.suggest.enabled": false,
  "clangd.enabled": false,
  "clangd.semanticHighlighting": true,
  "clangd.arguments": ["--background-index"],
  "latex.build.args": ["-pdf", "-interaction=nonstopmode", "-synctex=1", "%f"],
  "latex.build.outputDirectory": "build",
  "latex.build.onSave": true,
  "latex.lint.onChange": true,
  "latex.lint.onSave": false,
  "latex.forwardSearch.executable": "/Applications/Skim.app/Contents/SharedSupport/displayline",
  "latex.forwardSearch.args": ["-g", "%l", "%p", "%f"],
  "python.linting.pylintArgs": ["--generated-members=numpy.* ,torch.*", "--disable=C0111,C0103", ""]
}
